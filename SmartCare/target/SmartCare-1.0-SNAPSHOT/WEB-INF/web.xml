<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>dbname</param-name>
        <param-value>SmartCare_4</param-value>
    </context-param>
    <context-param>
        <param-name>docSalary</param-name>
        <param-value>9.0</param-value>
    </context-param>
    <context-param>
        <param-name>nurseSalary</param-name>
        <param-value>4.0</param-value>
    </context-param>
    <context-param>
        <param-name>clientCharge</param-name>
        <param-value>20.0</param-value>
    </context-param>
    <welcome-file-list>
        <welcome-file>/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Book</servlet-name>
        <jsp-file>book.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>PatientController</servlet-name>
        <servlet-class>controller_Login.PatientController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>employeeController</servlet-name>
        <servlet-class>controller_Login.employeeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewUser</servlet-name>
        <servlet-class>controller_Login.NewUserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller_Login.Login</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>OrganisationServlet</servlet-name>
        <servlet-class>controller.OrganisationServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>BookingSucessful</servlet-name>
        <jsp-file>BookingSucessful.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ListController</servlet-name>
        <servlet-class>controller_Login.ListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TurnoverController</servlet-name>
        <servlet-class>controller.TurnoverController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Book</servlet-name>
        <url-pattern>/Book.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewUser</servlet-name>
        <url-pattern>/NewUserController.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>OrganisationServlet</servlet-name>
        <url-pattern>/OrganisationServlet.do</url-pattern>
    </servlet-mapping>  
    <servlet-mapping>
        <servlet-name>OrganisationServlet</servlet-name>
        <url-pattern>/OrganisationServlet.do/new_organisation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrganisationServlet</servlet-name>
        <url-pattern>/OrganisationServlet.do/insert_organisation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrganisationServlet</servlet-name>
        <url-pattern>/OrganisationServlet.do/delete_organisation</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet-mapping>
        <servlet-name>BookingSucessful</servlet-name>
        <url-pattern>/BookingSucessful.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientController</servlet-name>
        <url-pattern>/PatientController.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>employeeController</servlet-name>
        <url-pattern>/employeeController.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListController</servlet-name>
        <url-pattern>/ListController.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TurnoverController</servlet-name>
        <url-pattern>/TurnoverController.do</url-pattern>
    </servlet-mapping>  
    

    <session-config>
        <session-timeout>
            20
        </session-timeout>
    </session-config>
</web-app>
